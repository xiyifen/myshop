<mapper namespace="com.xiyifen.myshop.system.mapper.GoodsAttrMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.xiyifen.myshop.system.entity.GoodsAttr">
                    <id column="id" property="id" />
                    <result column="goods_id" property="goodsId" />
                    <result column="attr_id" property="attrId" />
                    <result column="attr_value" property="attrValue" />
