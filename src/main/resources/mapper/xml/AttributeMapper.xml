<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiyifen.myshop.system.mapper.AttributeMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.xiyifen.myshop.system.entity.Attribute">
                    <id column="attr_id" property="attrId" />
                    <result column="attr_name" property="attrName" />
                    <result column="cat_id" property="catId" />
                    <result column="attr_sel" property="attrSel" />
                    <result column="attr_write" property="attrWrite" />
                    <result column="attr_vals" property="attrVals" />
                    <result column="delete_time" property="deleteTime" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            attr_id, attr_name, cat_id, attr_sel, attr_write, attr_vals, delete_time
        </sql>

</mapper>
